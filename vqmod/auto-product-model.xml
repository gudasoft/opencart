<modification>
	<id>Auto Model No For New Product in Admin</id>
    <version>2.0</version>
    <vqmver>2.6.6</vqmver>
    <author><![CDATA[<a onClick="window.open('http://www.gudasoft.com');" title="Gudasoft">Gudasoft</a>]]></author>


	<file name="admin/controller/catalog/product.php">
             <operation>
             <search position="replace"><![CDATA[$data['model'] = '';]]></search>
             <add><![CDATA[
			 $data['model'] = random_int(1000000000000, 9999999999999);
			 ]]></add>
             </operation>
    </file>
</modification>
